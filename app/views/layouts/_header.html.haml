%section{id: 'big_white_stripe_header'}
	%div{class: "container"}
		%div{class: "row",id:"new_page_header"}
			%div{class: "span2"}
				%div{id:"new_main_seal"}
					%div{id:"new_main_seal_title"}
						= link_to "Geekology", root_url
					%div{id:"new_main_seal_logo"}
						= link_to "GK", root_url
			%div{class: "span9", id: "new_nav-cluster"}
				%div{class: "new_sub_head_nav"}= link_to "Blog", "http://blog.geekology.biz"
				%div{class: "new_sub_head_nav"}= link_to "Contact", url_for('/#contact_link')
				%div{class: "new_sub_head_nav"}= link_to "Classes", url_for('/#choose_your_house_link')
				%div{class: "new_sub_head_nav"}= link_to "Why Geekology?", url_for('/#what_makes_different_link')
				
%section{id: 'big_white_stripe_header_2'}
	%div{class: "container"}
		%div{class: "row",id:"new_page_header_2"}
			%div{class: "span2"}
				%div{id:"new_main_seal"}
					%div{id:"new_main_seal_title"}
						= link_to "Geekology", root_url
					%div{id:"new_main_seal_logo"}
						= link_to "GK", root_url
				%div{id:"new_approve_seal"}
					=image_tag('hrci_preapproval_seal.png')
				
-#%section{id: 'sub_white_stripe_header'}
	%div{class: "container"}
		%div{class:"row", id: "new_sub_banner"}
			%div{class: "span2"}
				%div{id:"new_main_seal"}
					%div{id:"new_main_seal_title"}
						= link_to "School Of Geekology", root_url
					%div{id:"new_main_seal_logo"}
						= link_to "GK", root_url
				%div{id:"new_approve_seal"}
					=image_tag('hrci_preapproval_seal.png')
			%div{class: "span7 offset3", id: "new_nav-cluster"}
				%div{class: "new_head_nav"}= link_to "Contact", url_for('/#contact_link')
				%div{class: "new_head_nav"}= link_to "Blog", "http://blog.geekology.biz"
				%div{class: "new_head_nav"}= link_to "Classes", url_for('/#choose_your_house')

-#%section{class: 'old_header'}
	%div{class: "container"}
		%div{class: "row",id:"page_header"}
			%div{class: "span1"}
				%div{id:"sub_seal"}
					%div{id:"main_seal_logo"}
						= link_to "GK", root_url
		
			%div{class: "span4", id: "nav-cluster"}
				%div{class: "sub_head_nav"}= link_to "Contact", url_for('/#contact_link')
				%div{class: "sub_head_nav"}= link_to "Blog", "http://blog.geekology.biz"
				%div{class: "sub_head_nav"}= link_to "Classes", url_for('/#choose_your_house')
		
		%div{class:"row", id: "sub_banner"}
			%div{class: "span2"}
				%div{id:"main_seal"}
					%div{id:"main_seal_title"}
						= link_to "School Of Geekology", root_url
					%div{id:"main_seal_logo"}
						= link_to "GK", root_url
				%div{id:"approve_seal"}
					=image_tag('hrci_preapproval_seal.png')
			%div{class: "span7 offset3", id: "nav-cluster"}
				%div{class: "head_nav"}= link_to "Contact", url_for('/#contact_link')
				%div{class: "head_nav"}= link_to "Blog", "http://blog.geekology.biz"
				%div{class: "head_nav"}= link_to "Classes", url_for('/#choose_your_house')
		
		%div{class:"row", id: "vit_banner"}
			%div{class: "span12"}
